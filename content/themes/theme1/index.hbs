  <script>


    
                let currentServicesPage = 0;
                let visibleServices = [];
                let currentPartnersPage = 0;
                let visiblePartners = [];
                let selectedService = 0;

                window.onload=function(){
                    if(window.innerWidth > 650){
                    paginatePartners(true);
                    }
                    else{
                        currentPartnersPage = 0;
                        visiblePartners = [];
                        paginatePartners();
                    }
                    navTechnologies(1);
                    paginateServices(true);

                

                    window.addEventListener("orientationchange", function(event) {
                    if(window.innerWidth > 650){
                        let content1 = document.getElementsByClassName('contact_section-2--content-1--line');
                        let content2 = document.getElementsByClassName('contact_section-2--content-2--line');
                        let content3 = document.getElementsByClassName('contact_section-2--content-3--grid')[0];

                        for(let i = 0; i<content1.length; i++){
                        content1[i].style = '';
                        }
                        for(let i = 0; i<content2.length; i++){
                        content2[i].style = '';
                        }
                        content3.style = '';

                        navTechnologies(1);
                        paginateServices();
                        paginatePartners();
                    }
                    else{
                        navTechnologies(1);
                        currentPartnersPage = 0;
                        visiblePartners = [];
                        paginatePartners();
                        paginateServices();
                    }
                    }, true);

                    window.addEventListener('resize', function(event) {
                    if(window.innerWidth > 650){
                        let content1 = document.getElementsByClassName('contact_section-2--content-1--line');
                        let content2 = document.getElementsByClassName('contact_section-2--content-2--line');
                        let content3 = document.getElementsByClassName('contact_section-2--content-3--grid')[0];

                        for(let i = 0; i<content1.length; i++){
                        content1[i].style = '';
                        }
                        for(let i = 0; i<content2.length; i++){
                        content2[i].style = '';
                        }
                        content3.style = '';

                        navTechnologies(1);
                        paginateServices();
                        paginatePartners();
                    }
                    else{
                        navTechnologies(1);
                        paginateServices();
                        currentPartnersPage = 0;
                        visiblePartners = [];
                        paginatePartners();
                    }
                    }, true);


                    let navItems = document.getElementsByClassName('item');

                    if(window.location.pathname == '/{{locale}}/'){
                    if(window.location.hash != ''){
                        for(let i=0; i<navItems.length; i++){
                        navItems[i].classList.remove('selected')
                        }
                        let item = document.getElementById(`{{locale}}-item-${window.location.hash.replace('#','')}`);
                        if(item){
                        item.classList.add('selected');
                        }
                    }
                    else{
                        for(let i=0; i<navItems.length; i++){
                        navItems[i].classList.remove('selected')
                        }
                        let homeItem=document.getElementById(`{{locale}}-item-home`);
                        if(homeItem){homeItem.classList.add('selected');}
                    }
                    }
                    else{
                    for(let i=0; i<navItems.length; i++){
                        navItems[i].classList.remove('selected')
                        }
                    }

                
                };

 

                function captureAnalytics(eventName,data) {
                    if(eventName === 'contact'){
                        window.logEvent(window.analytics, eventName,
                        {
                                name: event.target.elements.name.value,
                                company: event.target.elements.company.value,
                                countryCode: event.target.elements.countryCode.value,
                                phone: event.target.elements.phone.value,
                                email: event.target.elements.email.value,
                                service: event.target.elements.service.value,
                                message: event.target.elements.message.value,
                        });
                    }
                    else if(eventName === 'e-invoice'){
                        window.logEvent(window.analytics, eventName,
                            {
                                    name: event.target.elements.name.value,
                                    company: event.target.elements.company.value,
                                    phone: '00962' + event.target.elements.phone.value,
                                    city: event.target.elements.city.value,
                                    email: event.target.elements.email.value,
                                    website: event.target.elements.website.value,
                                    message: event.target.elements.message.value,
                            });
                        }
                        else{
                            window.logEvent(window.analytics, eventName, { name: `${data}`});
                        }
                }

                function scrollHorizontally(className, left){
                    let el = document.getElementsByClassName(className)[0];


                    if(left){
                    if('{{locale}}' == 'en'){
                        el.scrollLeft -=window.innerWidth-30; 
                    }
                    else{
                        el.scrollLeft +=window.innerWidth-30; 
                    }
                    }
                    else if(left !=null){
                    if('{{locale}}' == 'en'){
                        el.scrollLeft +=window.innerWidth-30; 
                    }
                    else{
                        el.scrollLeft -=window.innerWidth-30; 
                    }
                    }

                    if(el.scrollLeft == 0 || el.scrollLeft == 1){
                    el.parentElement.getElementsByClassName('arrow-prev')[0].classList.remove('active');
                    el.parentElement.getElementsByClassName('arrow-prev')[0].onclick = null;
                    el.parentElement.getElementsByClassName('arrow-next')[0].classList.add('active');
                    el.parentElement.getElementsByClassName('arrow-next')[0].onclick = function(){scrollHorizontally('projects_content', false);};
                    }

                    if(el.scrollLeft > 0 && el.scrollLeft < el.scrollWidth && '{{locale}}' == 'en'){
                    el.parentElement.getElementsByClassName('arrow-prev')[0].classList.add('active');
                    el.parentElement.getElementsByClassName('arrow-next')[0].classList.add('active');
                    el.parentElement.getElementsByClassName('arrow-prev')[0].onclick = function(){scrollHorizontally('projects_content', true);};
                    el.parentElement.getElementsByClassName('arrow-next')[0].onclick = function(){scrollHorizontally('projects_content', false);};
                    }

                    if(el.scrollLeft >= el.scrollWidth - el.offsetWidth && '{{locale}}' == 'en'){
                    el.parentElement.getElementsByClassName('arrow-prev')[0].classList.add('active');
                    el.parentElement.getElementsByClassName('arrow-next')[0].classList.remove('active');
                    el.parentElement.getElementsByClassName('arrow-prev')[0].onclick = function(){scrollHorizontally('projects_content', true);};
                    el.parentElement.getElementsByClassName('arrow-next')[0].onclick = null;
                    }

                    if((el.scrollLeft * -1) > 1 && (el.scrollLeft * -1) < el.scrollWidth && '{{locale}}' == 'ar'){
                    el.parentElement.getElementsByClassName('arrow-prev')[0].classList.add('active');
                    el.parentElement.getElementsByClassName('arrow-next')[0].classList.add('active');
                    el.parentElement.getElementsByClassName('arrow-prev')[0].onclick = function(){scrollHorizontally('projects_content', true);};
                    el.parentElement.getElementsByClassName('arrow-next')[0].onclick = function(){scrollHorizontally('projects_content', false);};
                    }

                    if((el.scrollLeft * -1) >= (el.scrollWidth - el.offsetWidth - 1) && '{{locale}}' == 'ar'){
                    el.parentElement.getElementsByClassName('arrow-prev')[0].classList.add('active');
                    el.parentElement.getElementsByClassName('arrow-next')[0].classList.remove('active');
                    el.parentElement.getElementsByClassName('arrow-prev')[0].onclick = function(){scrollHorizontally('projects_content', true);};
                    el.parentElement.getElementsByClassName('arrow-next')[0].onclick = null;
                    }
                }

                function selectNav(nav){
                    let allNavs = nav.parentElement.children;
                    for(let i = 0; i < allNavs.length; i++){
                    allNavs[i].classList.remove('selected')
                    }

                    nav.classList.add('selected');
                    captureAnalytics('changed_nav', nav.innerHTML);
                }

                function navTechnologies(index){
                    let techs = document.getElementsByClassName('technologies_content--technology');
                    let navs = document.getElementsByClassName('technologies_content-nav--btn');
                    let maxIndex = window.innerWidth > 650 ? (index - 1) * 10 + 9 : (index - 1) * 10 + (8- (index - 1));
                    let minIndex = window.innerWidth > 650 ? (index - 1) * 10 : ((index - 1) * 10) - (index - 1);

                    for(let i = 0; i < navs.length; i++){
                        navs[i].classList.remove('selected');
                        if(i == index-1){
                        navs[i].classList.add('selected');
                        }
                    }

                    for(let j = 0; j < techs.length; j++){
                        if(j <= maxIndex && j >= minIndex){
                        techs[j].style.display="flex"
                        }
                        else{
                        techs[j].style.display="none"
                        }
                    }
                }

                    function navServices(next){
                    let services = document.getElementsByClassName('services_content--service');

                    if(services.length == 0){return;}

                    let arrowNext = services[0].parentElement.getElementsByClassName('arrow-next')[0];
                    let arrowPrev = services[0].parentElement.getElementsByClassName('arrow-prev')[0];

                    if(next){selectedService++;}
                    else if(next !=null){selectedService--;}

                    if(selectedService <= 0){selectedService = 1;}

                    if(selectedService > services.length){return;}

                    for(let i = 0; i < services.length; i++){
                    services[i].style.display="none";
                    if(i == selectedService-1){
                        services[i].style.display="flex";
                    }
                    }

                    if(selectedService == 1){
                    arrowNext.classList.add('active');
                    arrowPrev.classList.remove('active');
                    arrowNext.onclick = function(){paginateServices(true)};
                    arrowPrev.onclick = null;
                    }
                    else if(selectedService >= services.length){
                    arrowPrev.classList.add('active');
                    arrowNext.classList.remove('active');
                    arrowPrev.onclick = function(){paginateServices(false)};
                    arrowNext.onclick = null;
                    }
                    else{
                    arrowNext.classList.add('active');
                    arrowPrev.classList.add('active');
                    arrowNext.onclick = function(){paginateServices(true)};
                    arrowPrev.onclick = function(){paginateServices(false)};
                    }

                }

                function paginateServices(next = null){
                    if(window.innerWidth > 650){
                    if(next){
                    currentServicesPage++;
                    }
                    else if(next != null){
                    currentServicesPage--;
                    }

                    if(currentServicesPage == 0) currentServicesPage++;

                    let services = document.getElementsByClassName('services_content--service');

                    if(services.length == 0){return;}

                    let arrowNext = services[0].parentElement.getElementsByClassName('arrow-next')[0];
                    let arrowPrev = services[0].parentElement.getElementsByClassName('arrow-prev')[0];

                    for(let i = 0; i < services.length; i++){
                    if(i < ((currentServicesPage * 3)) && i > (((currentServicesPage -1) * 3) -1) && visibleServices.indexOf(services[i]) == -1){
                        visibleServices.push(services[i]);

                        if(visibleServices.length > 3){
                        visibleServices.shift();
                        }
                        if(i == services.length -1){
                        if(arrowNext){
                            arrowNext.classList.remove('active');
                            arrowNext.onclick=null;
                        }
                        }
                    }
                    else{
                        services[i].style.display="none";
                        }    
                    }
                    

                    for(let j = 0; j < visibleServices.length; j++){
                        visibleServices[j].style.display = "flex";
                    }

                    if(services.length > 3 && arrowNext && services[services.length - 1] != visibleServices[visibleServices.length - 1]){
                    arrowNext.classList.add('active');
                    arrowNext.onclick=function(){
                        paginateServices(true);
                        };
                    }

                    if(currentServicesPage == 1){
                    if(arrowPrev){
                        arrowPrev.classList.remove('active')
                        arrowPrev.onclick = null;
                    }
                    }
                    else{
                    if(arrowPrev){
                        arrowPrev.classList.add('active');
                        arrowPrev.onclick=function(){
                        paginateServices(false);
                        };
                    }
                    }
                    }
                    else{
                    navServices(next);
                    }
                }


                function paginatePartners(next = null){
                    if(next){
                    currentPartnersPage++;
                    }
                    else if(next != null){
                    currentPartnersPage--;
                    }

                    let partnersNum = window.innerWidth <= 650 ? 2 : 5;

                    if(currentPartnersPage == 0) currentPartnersPage++;

                    let partners = document.getElementsByClassName('projects_partners--partner');

                    if(partners.length == 0){return;}

                    let arrowNext = partners[0].parentElement.getElementsByClassName('arrow-next')[0];
                    let arrowPrev = partners[0].parentElement.getElementsByClassName('arrow-prev')[0];

                    for(let i = 0; i < partners.length; i++){
                    if(i < ((currentPartnersPage * partnersNum)) && i > (((currentPartnersPage -1) * partnersNum) -1) && visiblePartners.indexOf(partners[i]) == -1){
                        visiblePartners.push(partners[i]);

                        if(visiblePartners.length > partnersNum){
                        visiblePartners.shift();
                        }
                        if(i == partners.length -1){
                        if(arrowNext){
                            arrowNext.classList.remove('active');
                            arrowNext.onclick=null;
                        }
                        }
                    }
                    else{
                        partners[i].style.display="none";

                        }    
                    }
                    
                    for(let j = 0; j < visiblePartners.length; j++){
                        visiblePartners[j].style.display = "block";
                    }

                    if(partners.length > partnersNum && arrowNext && partners[partners.length - 1] != visiblePartners[visiblePartners.length - 1]){
                    arrowNext.classList.add('active');
                    arrowNext.onclick=function(){
                        paginatePartners(true);
                        };
                    }

                    if(currentPartnersPage == 1){
                    if(arrowPrev){
                        arrowPrev.classList.remove('active')
                        arrowPrev.onclick = null;
                    }
                    }
                    else{
                    if(arrowPrev){
                        arrowPrev.classList.add('active');
                        arrowPrev.onclick=function(){
                        paginatePartners(false);
                        };
                    }
                    }
                }



                function expandInfo(el){
                    if(window.innerWidth <= 650){
                    let nextSibling = el.nextElementSibling;
                    let allSiblings = [];

                    let arrow = el.getElementsByClassName('arrow')[0];

                    if(arrow){
                        arrow.style.transform = arrow.style.transform == 'none' ? 'scaleY(-1) translateY(20%)' : 'none';
                    }

                    while(nextSibling) {
                        allSiblings.push(nextSibling);
                        nextSibling = nextSibling.nextElementSibling;
                    }

                    for(let i = 0; i<allSiblings.length; i++){
                        allSiblings[i].style.display = allSiblings[i].style.display == 'none' || allSiblings[i].style.display == '' ? el.className.includes('content-3') ? 'grid' : 'flex' : 'none';
                    }
                    }
                }


                function openSidebar(){
                    document.getElementsByClassName('sidebar')[0].classList.add('show');
                    document.getElementsByClassName('sidebar')[0].classList.remove('hide');
                }
                function closeSidebar(){
                    document.getElementsByClassName('sidebar')[0].classList.remove('show');
                    document.getElementsByClassName('sidebar')[0].classList.add('hide');
                }
            </script>
           